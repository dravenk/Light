{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 760,
        "resizable": true,
        "title": "light",
        "width": 1080
      }
    ],
    "withGlobalTauri": true
  },
  "build": {
    "beforeBuildCommand": "trunk build",
    "beforeDevCommand": "trunk serve",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "com.light.dev",
  "plugins": {
    "all": false,
    "fs": {
      "all": true,
      "copyFile": true,
      "createDir": true,
      "exists": true,
      "readDir": true,
      "readFile": true,
      "removeDir": true,
      "removeFile": true,
      "renameFile": true,
      "scope": [
        "$APP/*",
        "$APP"
      ],
      "writeFile": true
    },
    "shell": {
      "all": false,
      "open": true
    }
  },
  "productName": "light",
  "version": "0.1.1"
}